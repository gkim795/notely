{
  "name": "Notes Fetcher",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Retrieves Notes for whichever site you are on",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "permissions": ["activeTab"],
   "web_accessible_resources" : ["*.html"],
   "icons": {
     "128": "icon.png"
   }
}

